.log-list {
  display: flex;
  flex-direction: column;
  background-color: #fff;
}

.Cre-describe {
  height: 5em;
}

.Cre-chum {
  height: 5em;
}

.Cre-homeimg {
  margin-bottom: 15px;
}

.Cre-btn {
  margin-top: 0;
}

.org_box {
  height: 3em;
  line-height: 3em;
  padding-left: 1em;
  padding-right: 1em;
  background: #d6febe;
  position: relative;
  top: -4em;
  left: -5.3em;
  font-size: 0.7em;
  border-radius: 10%;
}
.item-inline {
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.item-inline text{
  padding-right: 20px;
  width: 112px;
}
.org_bot_cor {
  width: 0;
  height: 0;
  font-size: 0;
  border-width: 15px;
  border-style: solid;
  border-color: #d6febe transparent transparent;
  _border-color: #d6febe white white;
  overflow: hidden;
  position: absolute;
  left: 20px;
  bottom: -30px;
}

.min_width_input {
  width: 30px;
  margin-left: 10px;
}

.inline {
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.selected{
  border:1px solid red;
  border-radius:0px;


}

/*省市区开始*/

.item-view{
    display: flex;
    align-items: center;
    margin: 0rpx 16rpx;
    position: relative;
}

.item-view text {
    font-size: 32rpx;
    color: #333333;
    width: 20%;
    margin: 36rpx 0rpx;
}

.item-view input, .item-view .choose-area {
    margin-left: 20rpx;
    font-size: 28rpx;
}

.item-view .address-text {
    font-size: 28rpx;
    color: #333333;
    margin-left: 20rpx;
    flex: 1;
    margin-right: 40rpx;
    line-height: 35rpx;
}

.item-view .jump-img {
  width: 30rpx;
  height: 30rpx;
  position:absolute;
  top:0;
  bottom:0;
  margin:auto;
  right:0;
}


.item-view .placeholderStyle,.item-view .choose-area{
    font-size: 28rpx;
    color: #999999;
}

.line-view {
    width: 100%;
    height: 2rpx;
    background: #ddd;
}

.default-view {
    display: flex;
    align-items: center;
    justify-content: center;
}

.default-view text{
    font-size: 32rpx;
    color: #333333;
    margin: 36rpx 0rpx 36rpx 10rpx;
}

.bottom-button {
    display: flex;
    width: 100%;
    height: 100rpx;
    position: absolute;
    bottom: 0;
}

.bottom-button text {
    width: 50%;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    font-size: 32rpx;
}

.bottom-button .cancel {
    color: #ddd;
    background: #999999;
}

.bottom-button .save {
    color: #ddd;
    background: #f23030;
}
.create_time{
  margin-left:28px;
}


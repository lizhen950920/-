/*图片9宫格  */
/*画廊*/
.gallery {
    /*margin-bottom: 100rpx;*/
    display: flex;
    justify-content: flex-start;
    flex-wrap: wrap;
}

/*每张图片所占容器*/
.item {
    position: relative;
    margin: 5px;
}

/*删除按钮*/
.delete {
    position: absolute;
    height: 20px;
    bottom: 0;
    width: 100%;
    background: #ccc;
    opacity: .8;
    margin-bottom:2px;
}

.delete image {
    position: absolute;
    width: 20px;
    height: 20px;
}
<!--pages/Aiqilin/DestSelect.wxml-->
<view class='container'>
  <button type='warn' size='mini'>目标地点定位</button>
  <text>{{address}}</text>
  <map id="myMap" markers='{{markers}}' longitude="{{longitude}}" latitude="{{latitude}}" scale="16" show-location style="width: 100%; height: 300px;"></map>
  <button class='okbtn'>确定</button>

  
</view>